@{

    var db = Database.Open("EA");


    if (IsPost)
    {

        if (!Request.Form["diagram"].IsEmpty())
        {
            db.Execute("update EA_diagrams_custom set  diagram_data=@0 where id =@1", Request.Unvalidated.Form["diagram"].Replace('\n',' ').Replace("\\n"," "), Request.QueryString["id"]);

        }

        db.Close();
    }
}
